{
  "name": "project-starter-sample",
  "version": "0.0.0",
  "description": "a sample for the output of the project starter module",
  "main": "server/app.js",
  "scripts": {
    "test": "npm run lint && npm run jest",
    "jest": "jest",
    "babel": "babel server -d dist/server -s",
    "copy": "copyfiles package.json .env.defaults .env.schema dist",
    "build": "rimraf dist && npm run babel && npm run build-client && npm run copy",
    "build-client": "webpack --env=production --define process.env.NODE_ENV=\"'production'\"",
    "lint": "run-p lint-*",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "lint-server": "eslint server --fix",
    "lint-client": "eslint client --fix",
    "lint-tests": "eslint __tests__ --fix",
    "dev": "run-p client-dev server-dev",
    "server-dev": "nodemon --watch server --exec \"babel-node\" server/app.js",
    "client-dev": "webpack-dev-server --open",
    "prod": "npm run build && node dist/server/app.js",
    "dev-docker": "run-p client-dev-docker server-dev-docker",
    "client-dev-docker": "webpack-dev-server --host 0.0.0.0 --watch-poll",
    "server-dev-docker": "npm run server-dev -- -L"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noamokman/project-starter-sample.git"
  },
  "keywords": [],
  "private": true,
  "author": "Noam Okman <noamokman@gmail.com> (https://github.com/noamokman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noamokman/project-starter-sample/issues"
  },
  "homepage": "https://github.com/noamokman/project-starter-sample#readme",
  "engines": {
    "node": ">=7"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "notify": true,
    "collectCoverage": true,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^1.0.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-noamokman": "^6.0.1",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-localstorage-mock": "^1.2.0",
    "lint-staged": "^4.0.2",
    "loadable-components": "^0.2.0",
    "material-ui": "^0.18.7",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "opn-cli": "^3.1.0",
    "plop": "^1.8.0",
    "plur": "^2.1.2",
    "preload-webpack-plugin": "^1.2.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-collapse": "^4.0.2",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-auth-wrapper": "^2.0.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-form": "^7.0.3",
    "redux-mock-store": "^1.2.3",
    "redux-simple-promise": "^2.0.2",
    "redux-sockets": "^1.0.0",
