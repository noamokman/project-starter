    "bunyan": "^1.8.10",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "dotenv-extended": "^2.0.1",
    "email-address": "^1.2.2",
    "env-bunyan": "^0.1.0",
    "express": "^4.15.3",
    "express-async-router": "^0.1.12",
    "express-json-error-handler": "^1.0.0",
    "express-jwt": "^5.3.0",
    "express-mongoose-errors": "^0.2.0",
    "express-param-objectid": "^1.0.0",
    "express-static-gzip": "^0.3.0",
    "http-errors": "^1.6.1",
    "http-reject-empty": "^1.0.0",
    "in-production": "^1.0.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.9",
    "mongoose": "^4.11.4",
